<p id="notice"><%= notice %></p>
<div class="container mt-5">
  <h1>Users</h1>

  <%= will_paginate renderer: custom_paginate_renderer %>
  <table style="margin: 3rem auto;">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <%= render @users %>
    </tbody>
  </table>
  <%= will_paginate renderer: custom_paginate_renderer %>

  <br>

  <%= link_to 'New User', new_user_path %>
</div>
